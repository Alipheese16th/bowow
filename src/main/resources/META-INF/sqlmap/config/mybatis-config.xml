<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
    PUBLIC "-//mybatis.org//DTD config 3.0//EN"
    "https://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	<typeAliases>
		<!-- 김소연 시작 -->
		<typeAlias type="com.lec.bowow.model.Member" alias="Member"/>
		<typeAlias type="com.lec.bowow.model.Coupon" alias="Coupon"/>
		<typeAlias type="com.lec.bowow.model.Review" alias="Review"/>
		<typeAlias type="com.lec.bowow.model.ReviewComment" alias="ReviewComment"/>
		<!-- 김소연 끝 -->
		
		<!-- 유민상 시작 -->
		<typeAlias type="com.lec.bowow.model.Admin" alias="Admin"/>
		<typeAlias type="com.lec.bowow.model.Qna" alias="Qna"/>
		<typeAlias type="com.lec.bowow.model.Faq" alias="Faq"/>
		<typeAlias type="com.lec.bowow.model.Inquiry" alias="Inquiry"/>
		<!-- 유민상 끝 -->
		
		<!-- 신치윤 시작 -->
		<typeAlias type="com.lec.bowow.model.Product" alias="Product"/>
		<typeAlias type="com.lec.bowow.model.Sizes" alias="Sizes"/>
		<typeAlias type="com.lec.bowow.model.Color" alias="Color"/>
		<typeAlias type="com.lec.bowow.model.Image" alias="Image"/>
		<typeAlias type="com.lec.bowow.model.Cart" alias="Cart"/>
		<typeAlias type="com.lec.bowow.model.Order" alias="Order"/>
		<typeAlias type="com.lec.bowow.model.OrderDetail" alias="OrderDetail"/>
		<typeAlias type="com.lec.bowow.model.Notice" alias="Notice"/>
		<typeAlias type="com.lec.bowow.model.NoticeComment" alias="NoticeComment"/>
		<!-- 신치윤 끝 -->

	</typeAliases>
	<mappers>
		<!-- 김소연 시작 -->
		<mapper resource="META-INF/sqlmap/mappers/Member.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/Review.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/ReviewComment.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/Coupon.xml"/>
		<!-- 김소연 끝 -->
		
		<!-- 유민상 시작 -->
		<mapper resource="META-INF/sqlmap/mappers/Admin.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/Qna.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/Faq.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/Inquiry.xml"/>
		<!-- 유민상 끝 -->
		
		<!-- 신치윤 시작 -->
		<mapper resource="META-INF/sqlmap/mappers/Product.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/Cart.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/Order.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/Notice.xml"/>
		<mapper resource="META-INF/sqlmap/mappers/NoticeComment.xml"/>
		<!-- 신치윤 끝 -->
	</mappers>
</configuration>

